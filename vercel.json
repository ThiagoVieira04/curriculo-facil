{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "alias": [
    "curriculum-facil.vercel.app"
  ]
}